    public void parseFinishYear(Document html, Serial serial) {

    }


    public void parseSeasons(Document html, Serial serial) {

    }


    public void parseCreators(Document html, Serial serial) {
        List<String> creators = html.getElementsByAttributeValue("itemprop", "director").stream()
                .map(Element::text)
                .collect(Collectors.toList());
        serial.setCreatorList(creators);
    }


    public void parseScreenWriter(Document html, Serial serial) {
        List<String> screenwriters = html.getElementsByAttributeValue("itemprop", "creator").stream()
                .map(Element::text)
                .collect(Collectors.toList());
        serial.setScreenwriterList(screenwriters);
    }


    public void parseComposers(Document html, Serial serial) {
        List<String> composers = html.getElementsByAttributeValue("itemprop", "musicBy").stream()
                .map(Element::text)
                .collect(Collectors.toList());
        serial.setComposerList(composers);
    }


